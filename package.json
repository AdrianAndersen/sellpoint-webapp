{
  "name": "sellpoint-webapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "DATABASE_URL=$(heroku config:get DATABASE_URL -a sellpoint-db) next dev",
    "dev:test": "next dev",
    "build": "next build",
    "start": "DATABASE_URL=$(heroku config:get DATABASE_URL -a sellpoint-db) next start",
    "start:test": "next start",
    "lint": "yarn run lint:prettier && yarn run lint:js && yarn run lint:css",
    "lint:js": "eslint . --ext ts,tsx --ignore-path .prettierignore --fix",
    "lint:css": "stylelint \"**/*.css\" --fix --ignore-path .prettierignore",
    "lint:prettier": "prettier --check .",
    "prettier": "prettier --write .",
    "test": "jest"
  },
  "dependencies": {
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@prisma/client": "^2.19.0",
    "@react-google-maps/api": "2.1.1",
    "autoprefixer": "10.2.5",
    "classnames": "2.2.6",
    "moment": "2.29.1",
    "next": "10.0.9",
    "next-images": "1.7.0",
    "postcss": "8.2.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "swr": "0.5.3",
    "tailwindcss": "2.0.3"
  },
  "devDependencies": {
    "@types/node": "14.14.35",
    "@types/react": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "cypress": "6.8.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "prisma": "2.19.0",
    "stylelint": "13.12.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "typescript": "4.1.3"
  }
